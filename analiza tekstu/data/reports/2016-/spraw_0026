ZADANIE 12. ---------------------------------------------------------------

sss.java:

import java.rmi.*;
import java.rmi.registry.*;
import java.rmi.server.*;
import java.io.*;

public class sss {
  public static void main(String args[]) {
    try {
        String name = "factory";
        FactoryClass object = new FactoryClass();
        UnicastRemoteObject.exportObject(object);
		Naming.rebind(name, object);
    } catch(Exception e) {
		System.err.println("sss: blad !!! "+e.getMessage());
    }
  }
}


///////////////////////////////////////////////////////////////////////

BufferInterface.java:

import java.rmi.*;

public interface BufferInterface extends Remote {
	public void push(int elem) throws RemoteException;
	public int pop() throws RemoteException;
}


////////////////////////////////////////////////////////////////

cons.java:

import java.rmi.*;
import java.rmi.registry.*;

public class cons{
	public static void main(String args[]) {
        try{            
            BufferInterface buffer = (BufferInterface)Naming.lookup("rmi://localhost/nametest");
            for(int i=0; i < 1000; i++) {
				System.out.println("popped: "+buffer.pop());
			}
        } catch(Exception e) {
			e.printStackTrace();
		}
    }
}

///////////////////////////////////////////////////////////////////////

FactoryClass.java:

import java.rmi.*;
import java.io.*;
import java.rmi.registry.*;
import java.rmi.server.*;

public class FactoryClass implements FactoryInterface, Serializable {
	public BufferClass newBuffer() throws RemoteException {
		BufferClass buff = new BufferClass();
		return buff;
	}
	public void newNamedBuffer(String name) throws RemoteException {
		BufferClass buff = new BufferClass();
		try{
			UnicastRemoteObject.exportObject(buff);
			Naming.rebind(name, buff);
		} catch(Exception e) {
			System.err.println("sss: blad !!! " + e.getMessage());
		}
	}

}
/////////////////////////////////////////////////////////////////////

FactoryInterface.java:

import java.rmi.*;

public interface FactoryInterface extends Remote {
	public BufferClass newBuffer() throws RemoteException;
	public void newNamedBuffer(String name) throws RemoteException;
}

////////////////////////////////////////////////////////////////////////////

kkk.java:

import java.rmi.*;
import java.rmi.registry.*;

public class kkk {
	public static void main(String args[]) {
		try {
			FactoryInterface obj = (FactoryInterface)Naming.lookup("rmi://localhost/factory");
			obj.newNamedBuffer("nametest");
		} catch(Exception e) {
			System.err.println("kkk: blad !!! " + e.getMessage());
		}
	}
}
////////////////////////////////////////////////////////////////////////////////

prod.java:

import java.rmi.*;

public class prod {
    public static void main(String args[]) {
        try{
            BufferInterface buffer = (BufferInterface)Naming.lookup("rmi://localhost/nametest");
            for(int i = 1; i <= 1000; i++) {
				buffer.push(i);
				System.out.println("pushed: " + i);
			}
        } catch(Exception e) {
			e.printStackTrace();
		}
    }
}
