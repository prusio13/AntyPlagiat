	Kod klienta:
		
	/*
	eksperymenty z RMI (klient)
	*/

	import java.rmi.*;
	import java.rmi.registry.*;

	public class kkk
	{
	  public static void main(String args[])
	  {
		//System.setSecurityManager(new RMISe;
		  System.out.println("kkk: oo2.powolnaMetoda()=curityManager());

		try {
		  OdleglyObiekt oo = (OdleglyObiekt)Naming.lookup(
			"rmi://localhost/przyklad_rmi4");

		  System.out.println("kkk: oo.powolnaMetoda()="+oo.powolnaMetoda());

		  OdleglyObiekt oo2=oo.drugiObiekt();
		  System.out.println("kkk: oo2.powolnaMetoda()="+oo2.powolnaMetoda())"+oo2.powolnaMetoda());
		  oo.generujeBlad();

		} catch(Exception e) {
		  System.err.println("kkk: blad !!! "+e.getMessage());
		}
	  }
	}

	Do serwera dodane:
	public void generujeBlad() throws RemoteException
	{
		throw new RemoteException("sztucznie wygenerowany blad ...");
	}
	
	
	Interfejs:
		
	/*
	eksperymenty z RMI (def zdalnego interfejsu)
	*/

	import java.rmi.*;

	public interface OdleglyObiekt extends Remote
	{
	  OdleglyObiekt drugiObiekt() throws RemoteException;
	  int powolnaMetoda() throws RemoteException;
	  public void generujeBlad() throws RemoteException;
	}
Zadanie 11. --------------------------------------------------------------------------------------------------------------------------------

SERWER =====================================================

sss.java:

import java.rmi.*;
import java.rmi.registry.*;
import java.rmi.server.*;
import java.io.*;

public class sss {
	public static void main(String args[]) {
		try {
			FactoryClass obj = new FactoryClass();
			UnicastRemoteObject.exportObject(obj);
			Naming.rebind("factory", obj);
		} catch(Exception e) {
			System.err.println("sss: blad !!! " + e.getMessage());
		}
	}
}

///////////////////////////////////////
BufferClass.java:

import java.rmi.*;
import java.io.*;

public class BufferClass implements BufferInterface, Serializable {
	int[] array1 = new int[50];
	int tail1 = 0;
	int head1 = 0;
	int size1 = 0;
	public synchronized void push(int element) throws RemoteException {
		try {
			while (size1 == array1.length) {
				wait();
			}
			notifyAll();
		} catch(InterruptedException e) {
			e.printStackTrace();
		}
		array1[tail1] = element;
		tail1 = (tail1 + 1) % array1.length; 
		size1++;
		System.out.println("pushed element: " + element);
	}
	public synchronized int pop() throws RemoteException {
		try {
			while (size1==0) {
				wait();
			}
		notifyAll();
		}
		catch(InterruptedException e){
			e.printStackTrace();
		}
		int temp = array1[head1];
		head1 = (head1 + 1) % array1.length;
		size1--;
		return temp;
	}
}

///////////////////////////////////////

BufferInterface.java:

import java.rmi.*;

public interface BufferInterface extends Remote {
	public void push(int element) throws RemoteException;
	public int pop() throws RemoteException;
}

//////////////////////////////////////

FactoryClass.java:

import java.rmi.*;
import java.io.*;

public class FactoryClass implements FactoryInterface, Serializable {
	public BufferClass newBuffer() throws RemoteException {
		BufferClass buffer = new BufferClass();
		return buffer;
	}
}
/////////////////////////////////////

FactoryInterface.java:

import java.rmi.*;

public interface FactoryInterface extends Remote {
	public BufferClass newBuffer() throws RemoteException;
}

//////////////////////////////////////

KLIENT  ========================================

ProducerInterface.java:

import java.rmi.*;

public interface ProducerInterface {
    public void run();
}

///////////////////////////////////////

ConsumerClass.java:

import java.rmi.*;

public class ConsumerClass implements ConsumerInterface, Runnable {
    private BufferClass buffer;
    public ConsumerClass(BufferClass b) {
        this.buffer = b;
    }
    @Override
    public void run() {
        for (int i = 0; i < 1000; i++) {
            try {
				int temp = buffer.pop();
				System.out.println("popped element: " + temp);
            } catch (RemoteException e) {
                e.printStackTrace();
            }
        }
    }
}

//////////////////////////////////////////

ConsumerInterface.java:

import java.rmi.*;

public interface ConsumerInterface {
    public void run();
}

////////////////////////////////////////////

kkk.java:

import java.rmi.*;
import java.rmi.registry.*;

public class kkk {
	public static void main(String args[]) {
		try {
			FactoryInterface obj = (FactoryInterface)Naming.lookup("rmi://localhost/factory");
			BufferClass buffer = obj.newBuffer();
			ConsumerClass myConsumer = new ConsumerClass(buffer);
			ProducerClass myProducer = new ProducerClass(buffer);
			Thread consumerThread = new Thread(myConsumer);
			Thread producerThread = new Thread(myProducer);
			consumerThread.start();
			producerThread.start();
		} catch(Exception e) {
			System.err.println("kkk: blad !!! "+e.getMessage());
		}
	}
}

/////////////////////////////////////////

ProducerClass.java

import java.rmi.*;

public class ProducerClass implements ProducerInterface, Runnable {
    private BufferClass buffer;
    public ProducerClass(BufferClass b) {
        this.buffer = b;
    }
    @Override
    public void run() {
        for(int i = 0; i < 1000; i++) {
            try {
                buffer.push(i + 1);
            } catch (RemoteException e) {
                e.printStackTrace();
            }
        }
    }
}

