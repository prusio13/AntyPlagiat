1)
public int powolnaMetoda() throws RemoteException
  {
		L=L+1;
		System.out.println("sserver: powolnaMetoda(); L="+L);
		try 
		{
		Thread.sleep(20000);
		} 
		catch(Exception e) {};
		System.out.println("sserver: powolnaMetoda(); koniec dzialania!");
		return L;
  }
  
